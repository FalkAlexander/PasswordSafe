<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="BrowsingPanel" parent="AdwBin">
    <property name="vexpand">True</property>
    <property name="child">
      <object class="GtkStack" id="_stack">
        <child>
          <object class="GtkScrolledWindow" id="_scrolled_window">
            <property name="hscrollbar-policy">never</property>
            <child>
              <object class="GtkListView" id="_list_view">
                <style>
                  <class name="navigation-sidebar"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwStatusPage" id="_empty_group_box">
            <property name="icon_name">folder-symbolic</property>
            <property name="description" translatable="true">Add elements with the âž• button.</property>
            <property name="title" translatable="true">Empty Group</property>
          </object>
        </child>
        <child>
          <object class="AdwStatusPage" id="_empty_search_page">
            <property name="icon_name">folder-saved-search-symbolic</property>
            <property name="title" translatable="yes">No Search Results</property>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
